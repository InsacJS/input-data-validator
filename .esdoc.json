{
  "source": "./lib",
  "destination": "./docs/",
  "includes": ["\\.js$"],
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "undocumentIdentifier": { "enable": false },
        "unexportedIdentifier": { "enable": true },
        "lint": { "enable": true },
        "coverage": { "enable": true },
        "typeInference": { "enable": true },
        "brand": {
          "title": "Input Data Validator",
          "description": "Valida los datos de entrada de una ruta de un servicio web creado con express.",
          "repository": "https://github.com/waquispe/input-data-validator"
        }
      }
    }
  ]
}
