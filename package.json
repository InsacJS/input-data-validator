{
  "name": "input-data-validator",
  "version": "1.2.2",
  "description": "Valida los datos de entrada de una ruta de un servicio web creado con express.",
  "main": "index.js",
  "scripts": {
    "doc": "./node_modules/.bin/esdoc",
    "lint": "./node_modules/.bin/eslint ./lib",
    "test": "npm run test-unit && npm run test-integration",
    "test-unit": "NODE_ENV=test \"node_modules/.bin/mocha\" --recursive \"./test/unit/{,/**/}*.spec.js\"",
    "test-integration": "NODE_ENV=test \"node_modules/.bin/mocha\" --recursive \"./test/integration/{,/**/}*.spec.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/insacjs/input-data-validator.git"
  },
  "keywords": [
    "insac",
    "insacjs",
    "sequelize",
    "rest",
    "validator",
    "web",
    "service"
  ],
  "author": "Wilmer Alex Quispe Quispe <q.wilmer.alex@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/insacjs/input-data-validator/issues"
  },
  "homepage": "https://github.com/insacjs/input-data-validator#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "handlebars": "^4.0.11"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "esdoc": "^1.0.4",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.17.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "express": "^4.16.2",
    "field-creator": "^1.2.1",
    "mocha": "^5.0.0",
    "pg": "^7.4.1",
    "sequelize": "^4.32.7"
  }
